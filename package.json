{
  "name": "maz-ui",
  "version": "0.0.1",
  "description": "A stand-alone components library for Vue.Js & Nuxt.Js (v3.x)",
  "directories": {
    "lib": "packages/lib",
    "docs": "packages/docs",
    "testing": "packages/testing"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production'){process.exit(1)} \" || husky install",
    "pre-commit": "lint-staged",
    "format": "prettier . --write",
    "lint": "lerna run lint --parallel",
    "release": "npx lerna version"
  },
  "lint-staged": {
    "!(node_modules/**/*|packages/**/*|.prettierignore|.gitignore|.editorconfig).*": [
      "prettier . --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/command-exists": "^1.2.0",
    "@types/node": "^17.0.31",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^6.3.0",
    "gulp-conventional-changelog": "^2.0.35",
    "husky": "^8.0.1",
    "lerna": "^4.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisMazel/maz-ui.git"
  },
  "keywords": [
    "vue",
    "components",
    "vue-component",
    "framework",
    "library",
    "ui"
  ],
  "author": "LouisMazel <me@loicmazuel.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LouisMazel/maz-ui/issues"
  },
  "homepage": "https://louismazel.github.io/maz-ui/"
}
