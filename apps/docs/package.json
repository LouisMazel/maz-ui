{
  "name": "docs",
  "type": "module",
  "version": "4.1.8-beta.3",
  "private": true,
  "description": "documentation of maz-ui",
  "author": "LouisMazel <me@loicmazuel.com>",
  "license": "ISC",
  "scripts": {
    "dev": "vitepress dev",
    "build": "pnpm generate:components-docs && vitepress build",
    "generate:components-docs": "pnpm -F cli cli generate-components-docs",
    "build:test": "cross-env TEST_ENV=true pnpm build",
    "build:watch": "pnpm build:test --watch",
    "preview": "vitepress preview",
    "serve": "vitepress serve",
    "lint": "cross-env NODE_ENV=production eslint .",
    "lint:fix": "pnpm lint --fix",
    "ftp-deploy": "node ./ftp-deploy.mjs",
    "typecheck": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@maz-ui/eslint-config": "workspace:*",
    "@maz-ui/utils": "workspace:*",
    "chart.js": "^4.5.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "dayjs": "^1.11.19",
    "libphonenumber-js": "^1.12.33",
    "maz-ui": "workspace:*",
    "vitepress-plugin-google-analytics": "^1.0.2"
  },
  "devDependencies": {
    "@samkirkland/ftp-deploy": "^1.2.5",
    "@types/postcss-import": "^14.0.3",
    "@types/postcss-url": "^10.0.4",
    "@types/react": "^19.2.7",
    "@vercel/og": "^0.8.6",
    "postcss-import": "^16.1.0",
    "postcss-url": "^10.1.3",
    "react": "^19.2.3",
    "tailwindcss": "^3.4.17",
    "vitepress": "2.0.0-alpha.15",
    "vue": "^3.5.26"
  },
  "lint-staged": {
    "*.{js,ts,vue,mjs,mts,cjs,md,yml,json}": "cross-env NODE_ENV=production eslint --fix"
  }
}
