{
  "name": "docs",
  "type": "module",
  "version": "4.0.0-beta.21",
  "private": true,
  "description": "documentation of maz-ui",
  "author": "LouisMazel <me@loicmazuel.com>",
  "license": "ISC",
  "scripts": {
    "dev": "vitepress dev docs",
    "build": "pnpm generate:components-docs && vitepress build docs",
    "generate:components-docs": "pnpm -F cli cli generate-components-docs",
    "build:test": "cross-env TEST_ENV=true pnpm build",
    "build:watch": "pnpm build:test --watch",
    "preview": "vitepress preview docs",
    "serve": "vitepress serve docs",
    "lint": "cross-env NODE_ENV=production eslint .",
    "pre-commit": "lint-staged",
    "ftp-deploy": "node ./ftp-deploy.mjs",
    "typecheck": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@maz-ui/eslint-config": "workspace:*",
    "@maz-ui/utils": "workspace:*",
    "chart.js": "^4.5.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "dayjs": "^1.11.13",
    "libphonenumber-js": "^1.12.10",
    "maz-ui": "workspace:*",
    "vitepress-plugin-google-analytics": "^1.0.2"
  },
  "devDependencies": {
    "@samkirkland/ftp-deploy": "^1.2.4",
    "@types/postcss-import": "^14.0.3",
    "@types/postcss-url": "^10.0.4",
    "@types/react": "^19.1.8",
    "@vercel/og": "^0.8.5",
    "postcss-import": "^16.1.0",
    "postcss-url": "^10.1.3",
    "react": "^19.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "vitepress": "^2.0.0-alpha.8",
    "vue": "^3.5.18"
  },
  "lint-staged": {
    "*.{js,ts,vue,mjs,mts,cjs,md,yml,json}": "cross-env NODE_ENV=production eslint --fix"
  }
}
