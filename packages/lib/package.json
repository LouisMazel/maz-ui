{
  "name": "maz-ui",
  "version": "3.9.0",
  "description": "A standalone components library for Vue.Js 3 & Nuxt.Js 3",
  "author": "Louis Mazel <me@loicmazuel.com>",
  "main": "modules/maz-ui.umd.js",
  "module": "modules/maz-ui.mjs",
  "types": "maz-ui.d.ts",
  "files": [
    "components",
    "modules",
    "css",
    "maz-ui.d.ts",
    "tailwindcss/tailwind.config.js",
    "tailwindcss/utils",
    "tailwindcss/variables"
  ],
  "scripts": {
    "lint": "npm run lint:js && npm run lint:style",
    "lint:nofix": "cross-env NODE_ENV=production eslint --ext .ts,.js,.vue .",
    "lint:js": "cross-env NODE_ENV=production eslint --fix --ext .ts,.js,.vue .",
    "lint:style": "stylelint --fix \"./**/*.{vue,css,scss,postcss,pcss}\"",
    "format": "prettier . --write",
    "build": "ts-node build/vite.config.ts",
    "test:unit": "vitest",
    "test:unit:watch": "vitest watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:coverage:watch": "vitest watch --coverage",
    "test:unit:coverage:master": "vitest run --coverage --changed master",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*": "prettier -w -u",
    "*.{js,ts,vue}": "cross-env NODE_ENV=production eslint --fix",
    "*.{vue,css,scss,postcss,pcss}": "stylelint --fix"
  },
  "peerDependencies": {
    "chart.js": "^4.0.0",
    "dayjs": "^1.1.0",
    "dropzone": "^5.9.3",
    "libphonenumber-js": "^1.9.44",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "dropzone": {
      "optional": true
    },
    "libphonenumber-js": {
      "optional": true
    },
    "chart.js": {
      "optional": true
    },
    "vue-router": {
      "optional": true
    },
    "dayjs": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/dropzone": "^5.7.4",
    "@types/google.maps": "^3.52.6",
    "@types/node": "^18.16.0",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-vue": "^4.1.0",
