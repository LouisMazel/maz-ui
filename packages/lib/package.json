    "@vue/eslint-config-typescript": "^11.0.1",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.3.2",
    "autoprefixer": "^10.4.14",
    "c8": "^7.12.0",
    "chalk": "^4.1.2",
    "chart.js": "^4.2.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "dropzone": "^5.9.3",
    "eslint": "^8.39.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^46.0.0",
