{
  "name": "maz-ui",
  "type": "module",
  "version": "4.0.0-alpha.5",
  "description": "A standalone components library for Vue.Js 3 & Nuxt.Js 3",
  "author": "Louis Mazel <me@loicmazuel.com>",
  "license": "MIT",
  "homepage": "https://maz-ui.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisMazel/maz-ui.git"
  },
  "bugs": {
    "url": "https://github.com/LouisMazel/maz-ui/issues"
  },
  "keywords": [
    "vue",
    "nuxt",
    "components",
    "vue-component",
    "framework",
    "library",
    "ui",
    "nuxt",
    "nuxt3",
    "module",
    "nuxt-module"
  ],
  "sideEffects": [
    "*.css",
    "*.vue",
    "**/plugins/**",
    "**/directives/**",
    "**/composables/useToast*",
    "**/composables/useDialog*",
    "**/composables/useMountComponent*",
    "**/utils/scriptLoader*",
    "**/utils/userVisibility*"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/types/components/index.d.ts",
      "import": "./dist/components/index.mjs"
    },
    "./composables": {
      "types": "./dist/types/composables/index.d.ts",
      "import": "./dist/composables/index.mjs"
    },
    "./utils": {
      "types": "./dist/types/utils/index.d.ts",
      "import": "./dist/utils/index.mjs"
    },
    "./plugins": {
      "types": "./dist/types/plugins/index.d.ts",
      "import": "./dist/plugins/index.mjs"
    },
    "./directives": {
      "types": "./dist/types/directives/index.d.ts",
      "import": "./dist/directives/index.mjs"
    },
    "./formatters": {
      "types": "./dist/types/formatters/index.d.ts",
      "import": "./dist/formatters/index.mjs"
    },
    "./resolvers": {
      "types": "./dist/types/resolvers/index.d.ts",
      "import": "./dist/resolvers/index.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt/types.d.ts",
      "import": "./dist/nuxt/module.mjs"
    },
    "./components/*": {
      "types": "./dist/types/components/*.vue.d.ts",
      "import": "./dist/components/*.mjs"
    },
    "./composables/*": {
      "types": "./dist/types/composables/*.d.ts",
      "import": "./dist/composables/*.mjs"
    },
    "./utils/*": {
      "types": "./dist/types/utils/*.d.ts",
      "import": "./dist/utils/*.mjs"
    },
    "./plugins/*": {
      "types": "./dist/types/plugins/*.d.ts",
      "import": "./dist/plugins/*.mjs"
    },
    "./directives/*": {
      "types": "./dist/types/directives/*.d.ts",
      "import": "./dist/directives/*.mjs"
    },
    "./formatters/*": {
      "types": "./dist/types/formatters/*.d.ts",
      "import": "./dist/formatters/*.mjs"
    },
    "./resolvers/*": {
      "types": "./dist/types/resolvers/*.d.ts",
      "import": "./dist/resolvers/*.mjs"
    },
    "./icons": {
      "types": "./dist/types/icons/index.d.ts",
      "import": "./dist/icons/index.mjs"
    },
    "./tailwindcss": {
      "types": "./dist/types/tailwindcss/index.d.ts",
      "import": "./dist/tailwindcss/index.mjs"
    },
    "./tailwindcss/*": {
      "types": "./dist/types/tailwindcss/*.d.ts",
      "import": "./dist/tailwindcss/*.mjs"
    },
    "./styles": "./dist/css/main.css",
    "./aos-styles": "./dist/css/aos.css",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "bin": {
    "maz": "./bin/maz.mjs",
    "maz-ui": "./bin/maz.mjs"
  },
  "files": [
    "LICENSE",
    "README.md",
    "bin",
    "dist",
    "svg",
    "tailwindcss"
  ],
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    "maz": "maz",
    "lint": "pnpm lint:js && pnpm lint:style",
    "lint:nofix": "cross-env NODE_ENV=production eslint",
    "lint:js": "pnpm lint:nofix --fix",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:style": "stylelint --fix \"./**/*.{vue,css,scss,postcss,pcss,html}\"",
    "build": "pnpm typecheck && pnpm build:only",
    "build:only": "vite build",
    "build:dev": "vite build --watch",
    "build:analyze": "node --loader ts-node/esm build/analyze-bundle.ts",
    "test:unit": "vitest",
    "test:unit:watch": "vitest watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:coverage:watch": "vitest watch --coverage",
    "test:unit:coverage:master": "vitest run --coverage --changed master",
    "pre-commit": "lint-staged",
    "typecheck": "vue-tsc --noEmit --skipLibCheck"
  },
  "peerDependencies": {
    "@maz-ui/icons": "workspace:*",
    "@nuxt/kit": "^3.4.0",
    "chart.js": "^4.0.0",
    "dayjs": "^1.1.0",
    "defu": "^6.0.0",
    "libphonenumber-js": "^1.10.0",
    "nuxt": "^3.4.0",
    "unplugin-auto-import": ">=19.0.0 <20.0.0",
    "unplugin-vue-components": ">=28.0.0 <29.0.0",
    "valibot": ">=0.30.0 <2.0.0",
    "vue": "^3.5.0",
    "vue-router": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@maz-ui/icons": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "chart.js": {
      "optional": true
    },
    "dayjs": {
      "optional": true
    },
    "defu": {
      "optional": true
    },
    "libphonenumber-js": {
      "optional": true
    },
    "nuxt": {
      "optional": true
    },
    "unplugin-auto-import": {
      "optional": true
    },
    "unplugin-vue-components": {
      "optional": true
    },
    "valibot": {
      "optional": true
    },
    "vue-router": {
      "optional": true
    }
  },
  "dependencies": {
    "@maz-ui/cli": "workspace:*",
    "@maz-ui/themes": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@maz-ui/eslint-config": "workspace:*",
    "@maz-ui/icons": "workspace:*",
    "@nuxt/kit": "^3.17.5",
    "@nuxt/schema": "^3.17.5",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.2",
    "@vue/compiler-sfc": "^3.5.16",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "c8": "^10.1.3",
    "chalk": "5.4.1",
    "chart.js": "^4.4.9",
    "dayjs": "^1.11.13",
    "defu": "^6.1.4",
    "fs": "0.0.2",
    "glob": "catalog:",
    "jsdom": "^26.1.0",
    "json-templater": "^1.2.0",
    "libphonenumber-js": "catalog:",
    "minimist": "^1.2.8",
    "nuxt": "^3.17.5",
    "onchange": "^7.1.0",
    "path": "^0.12.7",
    "postcss": "^8.5.4",
    "postcss-html": "^1.8.0",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "postcss-replace": "^2.0.1",
    "postcss-simple-vars": "^7.0.1",
    "postcss-url": "^10.1.3",
    "rimraf": "^6.0.1",
    "sass": "^1.89.1",
    "semver": "^7.7.2",
    "sirv": "^3.0.1",
    "stylelint": "^16.20.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "stylelint-scss": "^6.12.0",
    "tailwindcss": "^3.4.16",
    "typescript": "catalog:",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "valibot": "^1.1.0",
    "vite": "^6.3.5",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "catalog:",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-plugin-static-copy": "^3.0.0",
    "vite-svg-loader": "^5.1.0",
    "vitest": "catalog:",
    "vitest-canvas-mock": "^0.3.3",
    "vue": "^3.5.16",
    "vue-chartjs": "^5.3.2",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.5.1",
    "vue-tsc": "catalog:"
  },
  "lint-staged": {
    "*.{js,ts,vue,mjs,mts,cjs}": "cross-env NODE_ENV=production eslint --fix",
    "*.{vue,css,scss,postcss,pcss}": "stylelint --fix --allow-empty-input"
  }
}
