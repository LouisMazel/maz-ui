{
  "name": "maz-ui",
  "version": "3.29.9",
  "description": "A standalone components library for Vue.Js 3 & Nuxt.Js 3",
  "author": "Louis Mazel <me@loicmazuel.com>",
  "main": "./modules/index.ts",
  "module": "./modules/index.ts",
  "types": "./types/modules/index.d.ts",
  "type": "module",
  "bin": {
    "maz": "./bin/maz.mjs",
    "maz-ui": "./bin/maz.mjs"
  },
  "exports": {
    ".": {
      "types": "./types/modules/index.d.ts",
      "import": "./modules/index.ts",
      "require": "./modules/index.ts"
    },
    "./components": {
      "types": "./types/components/index.d.ts",
      "default": "./components/index.ts"
    },
    "./resolvers": {
      "types": "./types/resolvers/index.d.ts",
      "import": "./resolvers/index.ts",
      "require": "./resolvers/index.ts"
    },
    "./components/*": {
      "types": "./types/components/*.vue.d.ts",
      "default": "./components/*"
    },
    "./nuxt": {
      "types": "./nuxt/types.d.ts",
      "import": "./nuxt/index.mjs",
      "require": "./nuxt/index.cjs"
    },
    "./styles": "./css/main.css",
    "./aos-styles": "./css/aos.css",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "files": [
    "components",
    "modules",
    "css",
    "tailwindcss",
    "nuxt",
    "types",
    "icons",
    "resolvers",
    "bin"
  ],
  "scripts": {
    "maz": "maz",
    "lint": "pnpm lint:js && pnpm lint:style",
    "lint:nofix": "cross-env NODE_ENV=production eslint --ext .ts,.js,.mjs,.mts,.cjs,.vue .",
    "lint:js": "cross-env NODE_ENV=production eslint --fix --ext .ts,.js,.mjs,.mts,.cjs,.vue .",
    "lint:style": "stylelint --fix \"./**/*.{vue,css,scss,postcss,pcss}\"",
    "format": "prettier . --write",
    "build": "pnpm type-check && node --loader ts-node/esm build/vite.config.ts",
    "get-icon-list": "node --loader ts-node/esm build/get-icon-list.ts",
    "test:unit": "vitest",
    "test:unit:watch": "vitest watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:coverage:watch": "vitest watch --coverage",
    "test:unit:coverage:master": "vitest run --coverage --changed master",
    "pre-commit": "lint-staged",
    "gen-vue-tsc": "vue-tsc --declaration --emitDeclarationOnly",
    "type-check": "tsc --noEmit -p tsconfig.json"
  },
  "lint-staged": {
    "*": "prettier -w -u",
    "*.{js,ts,vue,mjs,mts,cjs}": "cross-env NODE_ENV=production eslint --fix",
    "*.{vue,css,scss,postcss,pcss}": "stylelint --fix --allow-empty-input"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.4.0",
    "chart.js": "^4.0.0",
    "dayjs": "^1.1.0",
    "defu": "^6.0.0",
    "dropzone": "^5.9.3",
    "libphonenumber-js": "^1.9.44",
    "nuxt": "^3.4.0",
    "unplugin-vue-components": "^0.25.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "dropzone": {
      "optional": true
    },
    "libphonenumber-js": {
      "optional": true
    },
    "chart.js": {
      "optional": true
    },
    "vue-router": {
      "optional": true
    },
    "dayjs": {
      "optional": true
    },
    "nuxt": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "defu": {
      "optional": true
    },
    "unplugin-vue-components": {
      "optional": true
    }
  },
  "dependencies": {
    "@mazui/cli": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@nuxt/kit": "^3.8.2",
    "@nuxt/schema": "^3.8.2",
    "@types/dropzone": "^5.7.8",
    "@types/google.maps": "^3.54.10",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vitest/coverage-v8": "^1.0.1",
    "@vue/compiler-sfc": "^3.3.10",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "c8": "^8.0.1",
    "chalk": "4.1.2",
    "chart.js": "^4.4.1",
    "dayjs": "^1.11.10",
    "defu": "^6.1.3",
    "dropzone": "^5.9.3",
    "eslint-plugin-vue": "^9.19.2",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "fs": "0.0.2",
    "jsdom": "^23.0.1",
    "json-templater": "^1.2.0",
    "libphonenumber-js": "^1.10.51",
    "minimist": "^1.2.8",
    "nuxt": "^3.8.2",
    "onchange": "^7.1.0",
    "path": "^0.12.7",
    "postcss": "^8.4.32",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.0",
    "postcss-simple-vars": "^7.0.1",
    "postcss-url": "^10.1.3",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "replace-in-file": "^7.0.2",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "semver": "^7.5.4",
    "sirv": "^2.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.3.6",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.5",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-lib-inject-css": "^1.3.0",
    "vite-plugin-static-copy": "^1.0.0",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.0.1",
    "vitest-canvas-mock": "^0.3.3",
    "vue": "^3.3.10",
    "vue-chartjs": "^5.2.0",
    "vue-loader": "^17.3.1",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.25"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "bugs": {
    "url": "https://github.com/LouisMazel/maz-ui/issues"
  },
  "homepage": "https://louismazel.github.io/maz-ui-3/",
  "keywords": [
    "vue",
    "nuxt",
    "components",
    "vue-component",
    "framework",
    "library",
    "ui",
    "nuxt",
    "nuxt3",
    "module",
    "nuxt-module"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisMazel/maz-ui.git"
  },
  "license": "MIT"
}
