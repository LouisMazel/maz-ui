{
  "name": "maz-ui",
  "version": "3.6.11",
  "description": "A standalone components library for Vue.Js 3 & Nuxt.Js 3",
  "author": "Louis Mazel <me@loicmazuel.com>",
  "main": "modules/maz-ui.umd.js",
  "module": "modules/maz-ui.mjs",
  "types": "maz-ui.d.ts",
  "files": [
    "components",
    "modules",
    "css",
    "maz-ui.d.ts",
    "tailwindcss/tailwind.config.js",
    "tailwindcss/utils",
    "tailwindcss/variables"
  ],
  "scripts": {
    "lint": "npm run lint:js && npm run lint:style",
    "lint:nofix": "cross-env NODE_ENV=production eslint --ext .ts,.js,.vue .",
    "lint:js": "cross-env NODE_ENV=production eslint --fix --ext .ts,.js,.vue .",
    "format": "prettier . --write",
    "lint:style": "stylelint --custom-syntax postcss-html --fix \"./**/*.{vue,css,scss,postcss,pcss}\"",
    "clean": "rimraf dist types",
    "emit-types": "vue-tsc --declaration --emitDeclarationOnly",
    "build": "npm run clean && mkdir -p dist && npm run build:js && npm run build:css && cp package.json dist/package.json && npm run copy:tailwind",
    "build:watch": "onchange --await-write-finish 2000 'package/**/*' -e 'package/components/index.ts' --exclude-path .gitignore -- npm run build:modules",
    "build:lib": "ts-node build/vite.config.ts",
    "build:js": "npm run build:gen-components-entry && npm run build:lib && npm run emit-types && npm run build:file-types && npm run build:copy-comp-types && npm run build:components-entry",
    "build:css": "npm run build:tailwind && npm run build:scss",
    "build:scss": "ts-node build/compile-scss.ts",
    "build:tailwind": "tailwindcss -i package/tailwindcss/css/tailwind.css -o dist/css/main.css --config tailwind.config.js --postcss --minify",
    "copy:tailwind": "cp -R package/tailwindcss dist/tailwindcss",
    "build:modules": "npm run build:lib -- --package modules",
    "build:component": "npm run build:lib -- --component",
    "build:components": "npm run build:lib -- --package components",
    "build:file-types": "rollup --config build/rollup.types.config.ts --configPlugin @rollup/plugin-typescript",
    "build:copy-comp-types": "ts-node build/copy-components-types.ts",
    "build:components-entry": "ts-node build/build-components-entry.ts",
    "build:gen-components-entry": "ts-node build/generate-components-entry.ts",
    "test:unit": "vitest",
    "test:unit:watch": "vitest watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:coverage:watch": "vitest watch --coverage",
    "test:unit:coverage:master": "vitest run --coverage --changed master",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*": "prettier -w -u",
    "*.{js,ts,vue}": "cross-env NODE_ENV=production eslint --fix",
    "*.{vue,css,scss,postcss,pcss}": "stylelint --custom-syntax postcss-html --fix"
  },
  "peerDependencies": {
    "chart.js": "^3.0.0",
    "dayjs": "^1.1.0",
    "dropzone": "^5.9.3",
    "libphonenumber-js": "^1.9.44",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "dropzone": {
      "optional": true
    },
    "libphonenumber-js": {
      "optional": true
    },
    "chart.js": {
      "optional": true
    },
    "vue-router": {
      "optional": true
    },
    "dayjs": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/dropzone": "^5.7.4",
    "@types/google.maps": "^3.51.0",
    "@types/node": "^18.11.18",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.27.1",
    "@vue/compiler-sfc": "^3.2.39",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.1",
    "@vue/test-utils": "^2.2.7",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.11",
    "c8": "^7.12.0",
    "chalk": "^4.1.2",
    "chart.js": "^3.9.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "dropzone": "^6.0.0-beta.2",
    "eslint": "^8.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "eslint-plugin-vue": "^9.9.0",
    "fs": "0.0.2",
    "jsdom": "^21.0.0",
    "json-templater": "^1.2.0",
    "libphonenumber-js": "^1.10.18",
    "lint-staged": "^13.1.0",
    "onchange": "^7.1.0",
    "path": "^0.12.7",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.0",
    "postcss-simple-vars": "^7.0.1",
    "postcss-url": "^10.1.3",
    "prettier": "^2.8.2",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "replace-in-file": "^6.3.5",
    "rimraf": "^4.0.4",
    "rollup": "^3.10.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.57.1",
    "semver": "^7.3.8",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwind-css-variables": "^3.0.1",
    "tailwindcss": "^3.2.4",
    "terser": "^5.16.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-css-injected-by-js": "^2.3.1",
    "vitest": "^0.27.1",
    "vitest-canvas-mock": "^0.2.2",
    "vue": "^3.2.45",
    "vue-loader": "^17.0.1",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.24",
    "vuedraggable": "^2.24.3"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "bugs": {
    "url": "https://github.com/LouisMazel/maz-ui/issues"
  },
  "homepage": "https://louismazel.github.io/maz-ui-3/",
  "keywords": [
    "vue",
    "components",
    "vue-component",
    "framework",
    "library",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisMazel/maz-ui.git"
  },
  "license": "MIT"
}
