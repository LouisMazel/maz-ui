{
  "name": "@maz-ui/nuxt",
  "type": "module",
  "version": "4.3.4-beta.2",
  "description": "Nuxt module for Maz-UI",
  "author": "Louis Mazel <me@loicmazuel.com>",
  "license": "MIT",
  "homepage": "https://maz-ui.com/guide/nuxt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisMazel/maz-ui.git"
  },
  "bugs": {
    "url": "https://github.com/LouisMazel/maz-ui/issues"
  },
  "keywords": [
    "vue",
    "nuxt",
    "components",
    "vue-component",
    "framework",
    "library",
    "ui",
    "nuxt",
    "nuxt3",
    "module",
    "nuxt-module",
    "maz-ui"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "build": "pnpm dev && nuxt-module-build build",
    "dev": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "lint": "cross-env NODE_ENV=production eslint .",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "nuxi typecheck --skipLibCheck"
  },
  "peerDependencies": {
    "nuxt": ">=3.4.0 <5.0.0"
  },
  "dependencies": {
    "@maz-ui/themes": "workspace:*",
    "@maz-ui/translations": "workspace:*",
    "@nuxt/kit": "^4.2.2",
    "defu": "^6.1.4",
    "maz-ui": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.21.0",
    "nuxt": "^4.2.2"
  },
  "lint-staged": {
    "*.{js,ts,mjs,mts,cjs,md,yml,json}": "cross-env NODE_ENV=production eslint --fix"
  }
}
