{
  "name": "cli",
  "version": "3.7.1",
  "main": "./dist/cli.js",
  "bin": {
    "maz-cli": "./dist/cli.js"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint --ignore-path .gitignore --fix --ext .ts,.js .",
