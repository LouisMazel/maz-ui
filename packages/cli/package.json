{
  "name": "cli",
  "version": "3.29.12-beta.16",
  "main": "./dist/cli.mjs",
  "types": "./dist/cli.d.ts",
  "exports": {
    ".": "./dist/cli.mjs",
    "./cli": "./bin/cli.mjs",
    "./types": "./dist/cli.d.ts"
  },
  "bin": {
    "cli": "./bin/cli.mjs"
  },
  "type": "module",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint --ignore-path .gitignore --fix --ext .ts,.js,.mjs,.mts,.cjs .",
    "cli": "node --loader ts-node/esm src/cli.ts",
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "pre-commit": "lint-staged",
    "vue-docgen": "vue-docgen",
    "format": "prettier --ignore-path .gitignore . --write"
  },
  "lint-staged": {
    "*": "prettier -w -u",
    "*.{js,ts,mjs,mts,cjs}": "cross-env NODE_ENV=production eslint --fix"
  },
  "devDependencies": {
    "@clack/prompts": "^0.7.0",
    "@types/clear": "^0.1.4",
    "@types/figlet": "^1.5.8",
    "chalk": "4.1.2",
    "clear": "^0.1.0",
    "commander": "^11.1.0",
    "figlet": "^1.7.0",
    "vue-docgen-cli": "^4.67.0"
  }
}
