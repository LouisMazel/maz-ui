{
  "name": "@maz-ui/node",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.13.1",
  "description": "Node utilities for Maz-UI library",
  "author": "Louis Mazel <me@loicmazuel.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisMazel/maz-ui.git"
  },
  "bugs": "https://github.com/LouisMazel/maz-ui/issues",
  "keywords": [
    "node",
    "utilities",
    "maz-ui",
    "promise",
    "logger"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./*": "./src/*"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@maz-ui/utils": "workspace:*",
    "colorette": "^2.0.20"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,mts,cjs,md}": [
      "eslint --fix"
    ]
  }
}
