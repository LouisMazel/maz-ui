{
  "name": "testing",
  "version": "3.3.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --fix --ext .ts,.js,.vue .",
    "lint:style": "stylelint --custom-syntax postcss-html --fix \"**/*.{vue,css,scss,postcss,pcss}\"",
    "format": "prettier . --write",
    "preview": "vite preview",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*": "prettier -w -u",
    "*.{js,ts,vue}": "eslint --fix",
    "*.{vue,css,scss,postcss,pcss}": "stylelint --custom-syntax postcss-html --fix"
  },
  "dependencies": {
    "maz-ui": "file:../lib",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.16.0",
    "eslint-plugin-unicorn": "^45.0.1",
    "eslint-plugin-vue": "^9.8.0",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.0.0",
    "postcss-nested": "^6.0.0",
    "postcss-simple-vars": "^7.0.1",
    "postcss-url": "^10.1.3",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "sass": "^1.56.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwind-css-variables": "^3.0.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-eslint": "^1.8.1"
  }
}
