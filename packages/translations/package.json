{
  "name": "@maz-ui/translations",
  "type": "module",
  "version": "4.7.0-beta.0",
  "description": "Translations for Maz-UI library",
  "author": "Louis Mazel <me@loicmazuel.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisMazel/maz-ui.git"
  },
  "bugs": "https://github.com/LouisMazel/maz-ui/issues",
  "keywords": [
    "translations",
    "i18n",
    "internationalization",
    "localization",
    "maz-ui",
    "vue",
    "nuxt"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js"
    },
    "./locales": {
      "types": "./dist/locales/index.d.ts",
      "import": "./dist/locales/index.js"
    },
    "./locales/*": {
      "types": "./dist/locales/*.d.ts",
      "import": "./dist/locales/*.js"
    },
    "./composables": {
      "types": "./dist/composables/useTranslations.d.ts",
      "import": "./dist/composables/useTranslations.js"
    },
    "./composables/*": {
      "types": "./dist/composables/*.d.ts",
      "import": "./dist/composables/*.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch --mode development",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "cross-env NODE_ENV=production eslint .",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:unit:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vue": ">=3.5.0 <4.0.0"
  },
  "dependencies": {
    "@maz-ui/utils": "workspace:*"
  },
  "devDependencies": {
    "unbuild": "^3.6.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,mts,cjs,md,yml,json}": [
      "eslint --fix"
    ]
  }
}
