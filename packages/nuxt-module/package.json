{
  "name": "nuxt-module",
  "version": "3.20.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/module.ts"
    }
  },
  "main": "./src/module.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm prepare && nuxt-module-build --outDir=./../lib/dist/nuxt --failOnWarn=false",
    "prepare": "nuxt-module-build --stub && nuxt-module-build prepare",
    "dev": "nuxi dev playground",
    "dev:prepare": "pnpm prepare && nuxi prepare playground",
    "lint": "cross-env NODE_ENV=production eslint .",
    "format": "prettier --ignore-path .gitignore . --write",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "lint-staged": {
    "*": "prettier -w -u",
    "*.{js,ts}": "cross-env NODE_ENV=production eslint --fix"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.1",
    "maz-ui": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.0",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.3",
    "@nuxt/schema": "^3.8.1",
    "@nuxt/test-utils": "^3.8.1",
    "defu": "^6.1.3",
    "nuxt": "^3.8.1",
    "vitest": "^0.34.6"
  },
  "unbuild": {
    "failOnWarn": false
  }
}
