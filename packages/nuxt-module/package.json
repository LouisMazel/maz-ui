{
  "name": "nuxt-module",
  "version": "4.0.0-alpha.5",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/module.ts",
    "./*": "./*"
  },
  "main": "./src/module.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm prepare && nuxt-module-build build --outDir=./../lib/dist/nuxt --failOnWarn=false",
    "prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "dev": "nuxi dev playground",
    "dev:prepare": "pnpm prepare && nuxi prepare playground",
    "lint": "cross-env NODE_ENV=production eslint .",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --ignore-path .gitignore . --write",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "vue-tsc --noEmit --skipLibCheck --project tsconfig.check.json"
  },
  "lint-staged": {
    "*": "prettier -w -u",
    "*.{js,ts}": "cross-env NODE_ENV=production eslint --fix"
  },
  "dependencies": {
    "@nuxt/kit": "catalog:",
    "maz-ui": "workspace:*",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.4.1",
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "catalog:",
    "@nuxt/test-utils": "^3.19.1",
    "nuxt": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "unbuild": {
    "failOnWarn": false
  }
}
