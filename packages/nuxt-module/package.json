{
  "name": "nuxt-module",
  "version": "4.0.0-alpha.5",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/module.ts"
    }
  },
  "main": "./src/module.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm prepare && nuxt-module-build build --outDir=./../lib/dist/nuxt --failOnWarn=false",
    "prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "dev": "nuxi dev playground",
    "dev:prepare": "pnpm prepare && nuxi prepare playground",
    "lint": "cross-env NODE_ENV=production eslint .",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --ignore-path .gitignore . --write",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit --skipLibCheck --project tsconfig.check.json"
  },
  "lint-staged": {
    "*": "prettier -w -u",
    "*.{js,ts}": "cross-env NODE_ENV=production eslint --fix"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.4",
    "maz-ui": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/eslint-config": "^0.7.6",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.17.4",
    "@nuxt/test-utils": "^3.19.1",
    "defu": "^6.1.4",
    "nuxt": "^3.17.4",
    "typescript": "5.6.2",
    "vitest": "^2.1.9"
  },
  "unbuild": {
    "failOnWarn": false
  }
}
