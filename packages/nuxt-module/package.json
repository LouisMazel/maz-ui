{
  "name": "@maz-ui/nuxt",
  "version": "4.0.0-alpha.5",
  "license": "MIT",
  "type": "module",
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "build": "pnpm prepare && nuxt-module-build build",
    "prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "dev": "nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "cross-env NODE_ENV=production eslint .",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --ignore-path .gitignore . --write",
    "typecheck": "vue-tsc --noEmit --skipLibCheck --project tsconfig.check.json",
    "release": "npm run lint && npm run prepack && changelogen --release && npm publish && git push --follow-tags"
  },
  "lint-staged": {
    "*": "prettier -w -u",
    "*.{js,ts}": "cross-env NODE_ENV=production eslint --fix"
  },
  "dependencies": {
    "@nuxt/kit": "catalog:",
    "maz-ui": "workspace:*",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.4.1",
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "catalog:",
    "@nuxt/test-utils": "^3.19.1",
    "nuxt": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "unbuild": {
    "failOnWarn": false
  }
}
