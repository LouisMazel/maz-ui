{
  "name": "nuxt-module",
  "version": "3.45.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/module.ts"
    }
  },
  "main": "./src/module.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm prepare && nuxt-module-build build --outDir=./../lib/dist/nuxt --failOnWarn=false",
    "prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "dev": "nuxi dev playground",
    "dev:prepare": "pnpm prepare && nuxi prepare playground",
    "lint": "cross-env NODE_ENV=production eslint .",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --ignore-path .gitignore . --write",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "lint-staged": {
    "*": "prettier -w -u",
    "*.{js,ts}": "cross-env NODE_ENV=production eslint --fix"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.4",
    "maz-ui": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint-config": "^0.5.0",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.4",
    "@nuxt/test-utils": "^3.14.0",
    "defu": "^6.1.4",
    "nuxt": "^3.12.4",
    "vitest": "^2.0.5"
  },
  "unbuild": {
    "failOnWarn": false
  }
}
