{
  "name": "@maz-ui/utils",
  "type": "module",
  "version": "4.3.0",
  "description": "Utils of maz-ui for JavaScript/TypeScript users",
  "author": "Louis Mazel <me@loicmazuel.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisMazel/maz-ui.git"
  },
  "bugs": "https://github.com/LouisMazel/maz-ui/issues",
  "keywords": [
    "utils",
    "javascript",
    "typescript",
    "maz-ui"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "module": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./helpers": {
      "types": "./dist/types/helpers/index.d.ts",
      "import": "./dist/helpers/index.js",
      "module": "./dist/helpers/index.js",
      "default": "./dist/helpers/index.js"
    },
    "./helpers/*": {
      "types": "./dist/types/helpers/*.d.ts",
      "import": "./dist/helpers/*.js",
      "module": "./dist/helpers/*.js",
      "default": "./dist/helpers/*.js"
    },
    "./ts-helpers": "./dist/types/ts-helpers/index.d.ts",
    "./ts-helpers/*": "./dist/types/ts-helpers/*.d.ts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch --mode development",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "cross-env NODE_ENV=production eslint .",
    "lint:fix": "pnpm lint --fix",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:unit:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "libphonenumber-js": "^1.12.34",
    "vite": "^7.3.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,mts,cjs,md,yml,json}": [
      "eslint --fix"
    ]
  }
}
