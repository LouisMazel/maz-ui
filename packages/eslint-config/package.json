{
  "name": "@maz-ui/eslint-config",
  "type": "module",
  "version": "4.7.0",
  "description": "ESLint configuration for JavaScript/TypeScript projects",
  "author": "Louis Mazel <me@loicmazuel.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisMazel/maz-ui.git"
  },
  "bugs": "https://github.com/LouisMazel/maz-ui/issues",
  "keywords": [
    "eslint",
    "eslint-config",
    "javascript",
    "typescript",
    "code-quality",
    "maz-ui"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "cross-env NODE_ENV=production eslint .",
    "lint:fix": "pnpm lint --fix"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0 <10.0.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "eslint-plugin-format": "^1.4.0",
    "eslint-plugin-sonarjs": "^4.0.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-vuejs-accessibility": "^2.5.0"
  },
  "devDependencies": {
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "eslint": "^9.39.2",
    "unbuild": "^3.6.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,mts,cjs,md,yml,json}": [
      "eslint --fix"
    ]
  }
}
