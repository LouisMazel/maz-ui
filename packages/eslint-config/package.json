{
  "name": "@maz-ui/eslint-config",
  "type": "module",
  "version": "4.1.7-beta.5",
  "description": "ESLint configuration for JavaScript/TypeScript projects",
  "author": "Louis Mazel <me@loicmazuel.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisMazel/maz-ui.git"
  },
  "bugs": "https://github.com/LouisMazel/maz-ui/issues",
  "keywords": [
    "eslint",
    "eslint-config",
    "javascript",
    "typescript",
    "code-quality",
    "maz-ui"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "cross-env NODE_ENV=production eslint .",
    "lint:fix": "pnpm lint --fix",
    "pre-commit": "lint-staged"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-vuejs-accessibility": "^2.4.0"
  },
  "devDependencies": {
    "eslint": "^9.35.0",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,mts,cjs,md,yml,json}": [
      "eslint --fix"
    ]
  }
}
