{
  "name": "@maz-ui/icons",
  "type": "module",
  "version": "4.6.1",
  "description": "Icons set in SVG and Vue components",
  "author": "Louis Mazel <me@loicmazuel.com>",
  "license": "MIT",
  "homepage": "https://maz-ui.com/guide/icons",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisMazel/maz-ui.git"
  },
  "bugs": {
    "url": "https://github.com/LouisMazel/maz-ui/issues"
  },
  "keywords": [
    "vue",
    "nuxt",
    "components",
    "vue-component",
    "framework",
    "library",
    "ui",
    "nuxt",
    "nuxt3",
    "module",
    "nuxt-module",
    "maz-ui",
    "icons",
    "svg"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/static/*.d.ts",
      "import": "./dist/static/*.js"
    },
    "./static": {
      "types": "./dist/static/index.d.ts",
      "import": "./dist/static/index.js"
    },
    "./static/*": {
      "types": "./dist/static/*.d.ts",
      "import": "./dist/static/*.js"
    },
    "./lazy": {
      "types": "./dist/lazy/index.d.ts",
      "import": "./dist/lazy/index.js"
    },
    "./lazy/*": {
      "types": "./dist/lazy/*.d.ts",
      "import": "./dist/lazy/*.js"
    },
    "./resolvers": {
      "types": "./dist/resolvers.d.ts",
      "import": "./dist/resolvers.js"
    },
    "./icon-list": {
      "types": "./dist/icon-list.d.ts",
      "import": "./dist/icon-list.js"
    },
    "./svg/*": "./svg/*"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "svg"
  ],
  "scripts": {
    "build": "vite build",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "cross-env NODE_ENV=production eslint .",
    "lint:fix": "pnpm lint --fix"
  },
  "peerDependencies": {
    "unplugin-vue-components": ">=28.0.0 <32.0.0",
    "vue": ">=3.5.0 <4.0.0"
  },
  "peerDependenciesMeta": {
    "unplugin-vue-components": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@maz-ui/node": "workspace:*",
    "@maz-ui/utils": "workspace:*",
    "@vitejs/plugin-vue": "^6.0.4",
    "unbuild": "^3.6.1",
    "unplugin-vue-components": "^31.0.0"
  },
  "lint-staged": {
    "*.{js,ts,vue,mjs,mts,cjs,md,yml,json}": "cross-env NODE_ENV=production eslint --fix"
  }
}
