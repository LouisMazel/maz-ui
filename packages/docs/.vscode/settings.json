{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "eslint.validate": [
    "vue",
    "javascript",
    "typescript",
    "html"
  ],
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": true
  },
  "css.validate": false,
  "stylelint.validate": [
    // "vue",
    "css",
    "scss",
    "postcss",
    "pcss"
  ],
  "prettier.enable": true
}