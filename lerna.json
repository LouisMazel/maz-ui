{
  "packages": ["packages/*"],
  "loglevel": "verbose",
  "version": "3.0.12",
  "yes": false,
  "command": {
    "version": {
      "allowBranch": "release/*",
      "message": "chore(release): bump version to %s",
      "forceGitTag": true,
      "conventionalCommits": true,
      "includeMergedTags": true
    }
  },
  "$schema": "node_modules/lerna/schemas/lerna-schema.json"
}
