{
  "loglevel": "verbose",
  "version": "4.0.0-alpha.5",
  "command": {
    "version": {
      "allowBranch": [
        "master",
        "release/*",
        "refactor/next"
      ],
      "message": "chore(release): bump version %v",
      "conventionalCommits": true,
      "forceGitTag": false,
      "changelog": false,
      "yes": false,
      "push": false,
      "gitTagVersion": false,
      "tagVersionPrefix": "v",
      "commitHooks": true
    }
  },
  "npmClient": "pnpm",
  "$schema": "node_modules/lerna/schemas/lerna-schema.json"
}