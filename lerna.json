{
  "loglevel": "verbose",
  "version": "3.7.1",
  "yes": false,
  "npmClient": "pnpm",
  "useWorkspaces": true,
  "changelogPreset": "./lerna-changelogrc.js",
  "command": {
    "version": {
      "allowBranch": ["master", "chore/use-changelogen"],
      "message": "chore(release): bump version to %s",
      "conventionalCommits": true,
      "noCommitHooks": true,
      "noGitTagVersion": true,
      "noPush": true
    }
  },
  "$schema": "node_modules/lerna/schemas/lerna-schema.json"
}
